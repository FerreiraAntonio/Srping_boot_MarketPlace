public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println(toCamelCase("RWUBWUBWUBLWUB"));
		
	}
	static  String toCamelCase(String s){
		  

	    return Pattern.compile("WUBWUB").matcher(s).replaceAll("");// + Pattern.compile("WUB").matcher(s).replaceAll(" ") ;
	   // Matcher matcher 
	
}	}