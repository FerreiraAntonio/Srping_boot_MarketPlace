public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println(getCount("abracadabra"));
		
	}
	public static int getCount(String str) {
	    int vowelsCount = 0;
	  if(str!="a" || str!="e" || str!="i" || str!="o" || str!="u" ) {}
	  str.replaceAll("[a-u]","");
	  System.out.println(str);
	    return vowelsCount;
	  }
	
}