	public static boolean scramble(String str1, String str2) {
		
		
		
		for(int i=0; i< str1.length();i++) {
	//		for(int j=i+1; j< str1.length();j++) {
			
				for(char v : str2.toCharArray()) {
					//	System.out.println(str1.charAt(i)+" - "+v);
					if(str1.charAt(i)==v) {
						str1.charAt(i);
					}
					
			//	}
				
				
			}
		}
		
		    
	       return true;
	    }
	}