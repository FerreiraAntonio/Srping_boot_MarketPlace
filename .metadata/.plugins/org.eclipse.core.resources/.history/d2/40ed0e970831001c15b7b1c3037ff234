public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println(Tickets(new int[] {25, 25, 25, 100}));
		
	}
	public static String Tickets(int[] p) {
	  
	     int num=0;
		List<Integer> l = new ArrayList<>();
	
		String h= "";
		for(Integer a: p) {
			
		     l.add(a);    
			
			num+=a;
		}
		
	System.out.println((num )-(l.size()*25) +"entrou");
	if( (num )-(l.size()*25) >25) {
	
		h="NO";
	}	
	else {h= "YES";}
		
		
		return h;
	  }
	}