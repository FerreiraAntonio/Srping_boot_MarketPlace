public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println(GetSum(-1,2));
		
	}
   static	int d=0;
   static	int g=1;
	public static int GetSum(int a, int b) {
		if(a<0) {
		for(int i =1; i<b; i++) {
		g+=(i+1);
		System.out.println("i="+i+" i+1="+d+" ="+g	);
	//	
 } 
		b=g;}
	
		if(a != b) { return a+b;}
		else{ return a;}		
			
		
		}  }
