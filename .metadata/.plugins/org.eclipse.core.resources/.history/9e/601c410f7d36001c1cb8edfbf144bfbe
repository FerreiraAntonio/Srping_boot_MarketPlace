	System.out.println( twoSum(new int[] {1, 2, 3},4));
	}


	
	 public static int[] twoSum(int[] numbers, int target)
	    {
	     
		 
		 for(int i =0; i<numbers.length;i++) {
			 
			 for(int j =1;j<numbers.length;j++) {
				 
				 System.out.println(numbers[j]+" - "+ numbers[i]);
			 }
			 
		 }
		 
		 
		 return null; 
	    }
	}