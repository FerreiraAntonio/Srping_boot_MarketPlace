public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println( camelCase("camelCasingTilulo"));
	}
	

	 public static String camelCase(String a) {
		   	String[] b= a.toUpperCase().split("");
		    String[] g= a.split("");
	
		for(int i=0; i<b.length;i++) { 
			for(int j=0; j<g.length;j++) {
				 if(g[i].equals(b[j])) {
			String d="";
			d=a.substring(0, i)+" " +a.substring(i,a.length()); 		
				a=d;	 
			System.out.println(g[i]+" = "+b[j]+ "  i:"+i);
			break;
				// break;
				 } 
				
			}
		}
		
	//	 System.out.println(a);
		 return a;
		  }
		}