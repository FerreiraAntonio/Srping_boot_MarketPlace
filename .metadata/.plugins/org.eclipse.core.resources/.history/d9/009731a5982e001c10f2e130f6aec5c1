	public static String getCount(String str) {
	
		
		if(str.length() != 0 ) {	
		String[] f= str.split(" ");
		int num= f.length;
	for(int i=1;i<=f.length;i++) {
		System.out.println(i);
		
		for(int v=1;v<=f.length;v++) {
			String h = String.valueOf(v);
			int a= i-1;
			if(f[a].contains(h)&& (v)>i ) {
				System.out.println(f[i-1]+" Contem "+h);
				f[a].replace(f[a], f[v]);
				System.out.println(f[a]+ " - " + f[v]);
			}
			
	/*	if(f[v].contains(h)) {
			System.out.println("tem: "+f[v]);
		} */
	}
		}
			return null; }
		else {return "";}
		
		
	    
	  }
	
}