public class Testando {

	public static void main(String[] args) {
		
	
	System.out.println(GetSum( 4));
		
	}
   static	int  f=1;
	public static int GetSum( int x) {
		
	//	if(a != b) { return a+b;}
	//	else{ return a;}
		int d=0;
		for(int i =0; i<x; i++) {
			d = x*(x-1);
			x--;
			System.out.println(d);
		}
 return 0    ;} 

}
