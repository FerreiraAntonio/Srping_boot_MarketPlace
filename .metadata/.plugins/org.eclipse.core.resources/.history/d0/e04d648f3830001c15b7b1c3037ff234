	public static void main(String[] args) {
		
	
	System.out.println(toCamelCase("the-stealth-warrior"));
		
	}
	 static String toCamelCase(String s){
		    
		 
		 List a = new ArrayList<>();
		 
		 a.add(s.split("-"));
		String p="";
		
		for(Object f : a) {
		
		
		System.out.println(f) ;
		  } 
		return null;}
	
}	
