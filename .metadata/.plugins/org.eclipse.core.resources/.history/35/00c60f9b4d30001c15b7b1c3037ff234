	public static void main(String[] args) {
		
	
	System.out.println(toCamelCase("RWUBWUBWUBLWUB"));
		
	}
	static  String toCamelCase(String s){
		
		if(s.contains("WUBWUBWUB")) {
			System.out.println("caiu");
			s =s.replace("WUBWUBWUB"," ");}
		else {
			System.out.println("caiu else"); s =s.replaceAll("WUB","");}
		  
return s;
}	}