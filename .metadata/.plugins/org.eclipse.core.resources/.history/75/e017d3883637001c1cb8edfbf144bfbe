		public static  String Num(String num) {
			
		String r ="0";
			if(num.equals("one")) {
				r="1";
			}
			if(num.equals("two")) {
				System.out.println("entrou");
				r="2";
			}
			if(num.equals("three")) {
				r="3";
			}
			if(num.equals("four")) {
